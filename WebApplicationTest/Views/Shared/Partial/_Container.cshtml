@model IEnumerable<WebApplicationTest.Entities.Product>
<section>
    <div class="container">
        <div class="row">
            <div class="col-sm-3">
                @await Html.PartialAsync("Partial/_LeftSideBar")
            </div>
            <div class="col-sm-9 padding-right">
                @await Html.PartialAsync("Partial/ProductItems", Model)
                @await Html.PartialAsync("Partial/_CategoryTab")
                @await Html.PartialAsync("Partial/_RecommendedItems")
            </div>
        </div>
    </div>
</section>
